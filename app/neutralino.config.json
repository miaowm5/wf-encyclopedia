{
  "$schema": "https://raw.githubusercontent.com/neutralinojs/neutralinojs/main/schemas/neutralino.config.schema.json",
  "applicationId": "com.miaowm5.worldflipper",
  "version": "1.0.0",
  "defaultMode": "window",
  "port": 0,
  "documentRoot": "/resources/",
  "url": "/",
  "enableServer": true,
  "enableNativeAPI": false,
  "tokenSecurity": "one-time",
  "logging": { "enabled": false },
  "modes": {
    "window": {
      "title": "星见图鉴",
      "width": 414,
      "height": 896,
      "minWidth": 375,
      "minHeight": 667,
      "center": true,
      "fullScreen": false,
      "alwaysOnTop": false,
      "icon": "/resources/assets/appIcon.png",
      "enableInspector": false,
      "borderless": false,
      "maximize": false,
      "hidden": false,
      "resizable": true,
      "useSavedState": false,
      "exitProcessOnClose": true
    }
  },
  "cli": {
    "binaryName": "StarEncyclopedia",
    "resourcesPath": "/resources/",
    "extensionsPath": "/extensions/",
    "clientLibrary": "/resources/assets/neutralino.js",
    "binaryVersion": "6.5.0",
    "clientVersion": "6.5.0"
  }
}
