{
  {{#rects}}
  "{{{name}}}": {
    "frame": {
      "x": {{frame.x}},
      "y": {{frame.y}},
      "w": {{frame.w}},
      "h": {{frame.h}}
    },
    "spriteSourceSize": {
      "x": {{spriteSourceSize.x}},
      "y": {{spriteSourceSize.y}}
    },
    "sourceSize": {
      "w": {{sourceSize.w}},
      "h": {{sourceSize.h}}
    },
    "image": "{{config.imageName}}"
  }{{^last}},{{/last}}
  {{/rects}}
}
